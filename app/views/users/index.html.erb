<h1>All Users</h1>
<div id="page">
	<% @users.each do |u| %>
		<div class = 'userinfo'>
			<h4>Username:</h4> <%= link_to u.username, u %>
			<h4>First Name:</h4> <%= u.fname %>
			<h4>Last Name:</h4> <%= u.lname %>
			<h4>Avatar:</h4><%= image_tag u.avatar.url(:medium) %>
		</div>
	<% end %>
</div>